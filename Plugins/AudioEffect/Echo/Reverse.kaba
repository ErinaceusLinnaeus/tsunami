# Image = hui:back
# Title[Deutsch] = Rückwärts
use "../../api.kaba"

class Reverse extends AudioEffect
	override void process(AudioBuffer buf)
		int n = buf.length / 2
		for i in 0:n
			int j = buf.length - i - 1
			float x = buf.r[i]
			buf.r[i] = buf.r[j]
			buf.r[j] = x
			x = buf.l[i]
			buf.l[i] = buf.l[j]
			buf.l[j] = x

