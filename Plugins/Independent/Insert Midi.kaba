use "../api.kaba"

void main()
	MidiData m
	int dt = 20000
	int l = 19000
	m.add(0     , l, 69, 1)
	m.add(dt    , l, 71, 0.4)
	m.add(dt * 2, l, 72, 0.6)
	m.add(dt * 3, l, 74, 0.8)
	m.add(dt * 4, l, 76, 1)
	m.add(dt * 5, l, 81, 1)
	m.add(dt * 6, l, 76, 0.7)
	
	bool ok = false
	forall t in audio.track
		if t.type != TRACK_TYPE_MIDI
			continue
		
		t.InsertMidiData(0, m)
		ok = true
		break
	
	if !ok
		logging.Error("'Insert Midi.kaba': No midi track found")