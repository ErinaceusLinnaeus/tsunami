use "../api.kaba"

const string AutoConfigdelay = "-2:2:1:1000:ms"

class Config extends PluginData
	float delay
	
	override void reset()
		delay = 0

class Delay extends MidiEffect
	Config config
	
	override void process(MidiNoteBuffer midi)
		int dt = song.sample_rate * config.delay
		for n in midi
			n.range.offset += dt
