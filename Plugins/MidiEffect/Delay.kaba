use tsunami

const AUTO_CONFIG_DELAY = "-2:2:1:1000:ms"

class Config extends Module.Config
	var delay: float
	
	func override reset()
		delay = 0

class Delay extends MidiEffect
	var config: Config
	
	func override process(out midi: MidiEventBuffer)
		let dt = int(session.sample_rate() * config.delay)
		for e in midi
			e.pos += dt
