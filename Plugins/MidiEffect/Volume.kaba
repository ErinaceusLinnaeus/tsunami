use tsunami

const AUTO_CONFIG_VOLUME = "0:1:1:100:%"

class Config extends Module.Config
	var volume: float
	
	func override reset()
		volume = 1

class PitchShift extends MidiEffect
	var config: Config
	
	func override process(out midi: MidiEventBuffer)
		for e in midi
			if e.volume > 0
				e.volume = config.volume
