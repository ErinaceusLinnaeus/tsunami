Dialog bar_add_dialog "New bars" 250 80
	Grid ? "" 1 3
		Grid ? "" 2 3
			Text bad_t_count "Number"
			SpinButton count "1\\1\\10000\\1" expandx
			Text bad_t_beats "Beats"
			SpinButton beats "1\\1\\1000\\1" expandx
			Text bad_t_bpm "Bpm"
			SpinButton bpm "0\\0\\1000\\0.1" expandx
		Grid ? "" 2 1
			RadioButton insert:before "before"
			RadioButton insert:after "after"
		Grid ? "" 2 1 buttonbar
			Button /cancel "Cancel" image=hui:cancel
			DefButton /ok "OK" image=hui:ok
