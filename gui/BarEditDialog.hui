Dialog bar_edit_dialog "Edit bars" 250 80
	Grid ? "" 1 2
		Grid ? "" 2 3
			CheckBox edit_beats "Beats"
			SpinButton beats "1\\1\\1000\\1" expandx
			CheckBox edit_sub_beats "Sub beats"
			SpinButton sub_beats "1\\1\\24\\1" expandx
			CheckBox edit_bpm "Bpm"
			SpinButton bpm "0\\0\\1000\\0.1" expandx
		Grid ? "" 2 1 buttonbar
			Button /cancel "Cancel" image=hui:cancel
			DefButton /ok "OK" image=hui:ok
