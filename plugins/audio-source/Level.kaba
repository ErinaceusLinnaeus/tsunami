use tsunami
use helper.tone

let AUTO_CONFIG_LEVEL = "range=-1:1:0.01,scale=100,unit=%"


class FlipFlopSource extends AudioSource
	class Config extends Module.Config
		var level: float
		func override reset()
			level = 1

	var config: Config

	func override read(out buf: AudioBuffer) -> int
		for c in buf.c
			c = config.level
		return len(buf)
