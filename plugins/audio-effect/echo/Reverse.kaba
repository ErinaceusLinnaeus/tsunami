# Image = hui:back
# Title[Deutsch] = Rückwärts
use tsunami

class Reverse extends AudioEffect
	func override __init__()
		apply_to_whole_buffer = true

	func override process(out buf: AudioBuffer)
		var n = len(buf) / 2
		for c in buf.c
			for i in 0:n
				var j = len(buf) - i - 1
				var x = c[i]
				c[i] = c[j]
				c[j] = x

